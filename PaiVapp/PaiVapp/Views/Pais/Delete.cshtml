@model PaiVapp.Models.Pais

@{
    ViewData["Title"] = "Eliminar";
}

<h2>Eliminar Pais</h2>

<h3>¿Esta seguro de querer eliminar este Pais?</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NPais)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NPais)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PaisID" />
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        <a asp-action="Index"
           class="btn btn-default previous ">
            <span aria-hidden="true">&larr;</span>
            Volver
        </a>
    </form>
</div>
