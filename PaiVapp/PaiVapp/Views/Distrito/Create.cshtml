@model PaiVapp.Models.Distrito

@{
    ViewData["Title"] = "Nuevo";
}

<h2>Crear Distrito</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DepartamentoID" class="control-label">Departamento</label>
                <select asp-for="DepartamentoID" class="form-control" asp-items="ViewBag.DepartamentoID"></select>
            </div>
            <div class="form-group">
                <label asp-for="CodDistrito" class="control-label"> Cod Distrito</label>
                <input asp-for="CodDistrito" class="form-control" />
                <span asp-validation-for="CodDistrito" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NDistrito" class="control-label"> Distrito</label>
                <input asp-for="NDistrito" class="form-control" />
                <span asp-validation-for="NDistrito" class="text-danger"></span>
            </div>
          
          
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index"
       class="btn btn-default previous ">
        <span aria-hidden="true">&larr;</span>
        Volver
    </a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
