@model PaiVapp.Models.Edad

@{
    ViewData["Title"] = "Eliminar";
}
<h2>Eliminar Pais</h2>
<h3>¿Esta seguro de querer eliminar esta Edad?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NEdad)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NEdad)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Semanas)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Semanas)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Estado)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="EdadID" />
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        <a asp-action="Index"
           class="btn btn-default ">
            <span aria-hidden="true">&larr;</span>
            Volver
        </a>
    </form>
</div>