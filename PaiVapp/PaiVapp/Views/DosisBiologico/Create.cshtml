@model PaiVapp.Models.DosisBiologico
@{
    ViewData["Title"] = "Crear";
}
<h2>Crear Dosis por Biologico</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EdadID" class="control-label"> Edad en semanas</label>
                <select asp-for="EdadID" class="form-control" asp-items="ViewBag.EdadID"></select>
            </div>
            <div class="form-group">
                <label asp-for="BiologicoID" class="control-label">Biologico</label>
                <select asp-for="BiologicoID" class="form-control" asp-items="ViewBag.BiologicoID"></select>
            </div>
            <div class="form-group">
                <label asp-for="DosisID" class="control-label">Dosis</label>
                <select asp-for="DosisID" class="form-control" asp-items="ViewBag.DosisID"></select>
            </div>
            <div class="form-group">
                <label asp-for="Descripcion" class="control-label"></label>
                <input asp-for="Descripcion" class="form-control" />
                <span asp-validation-for="Descripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index"
       class="btn btn-default ">
        <span aria-hidden="true">&larr;</span>
        Volver
    </a>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
